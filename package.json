{
  "name": "@core-techs-git/pdb_requester",
  "version": "1.1.1",
  "description": "A request formater mmodule for pdb apps.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "analysis": "npm-run-all analysis:*",
    "analysis:circuit": "madge --circular src",
    "analysis:dependency": "check-audit",
    "analysis:lint": "eslint --ext .ts --color ./src",
    "analysis:style": "prettier --check src/**/*.ts",
    "build": "npm run clean && ttsc",
    "clean": "rimraf dist",
    "doc": "typedoc --tsconfig",
    "release": "bin/release",
    "test": "jest --runInBand --silent --colors",
    "watch": "npm run build -- -w"
  },
  "keywords": [
    "pdb",
    "requester"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/core-techs-git/pdb_requester.git"
  },
  "author": {
    "name": "Adimeo",
    "email": "contact@adimeo.com",
    "url": "https://adimeo.com/"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Core-Techs-Git/pdb_requester/issues"
  },
  "homepage": "https://github.com/Core-Techs-Git/pdb_requester#readme",
  "engines": {
    "node": "10.15.3",
    "npm": ">=6.9.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@types/config": "0.0.36",
    "@types/request": "^2.48.4",
    "config": "^3.2.6",
    "inversify": "^5.0.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "ssl-root-cas": "^1.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "@zerollup/ts-transform-paths": "^1.7.12",
    "auto-changelog": "^1.16.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "madge": "^3.7.0",
    "npm-audit-resolver": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "semver": "^7.1.3",
    "ts-jest": "^25.2.1",
    "ttypescript": "^1.5.10",
    "typedoc": "^0.16.11",
    "typedoc-plugin-external-module-name": "^3.0.0",
    "typescript": "^3.8.2"
  }
}
